  },
  "devDependencies": {
    "@lingui/cli": "^4.10.1",
    "@lingui/macro": "^4.10.1",
    "@lingui/swc-plugin": "^4.0.7",
    "@pandacss/dev": "^0.40.1",
    "@rspack/cli": "^0.7.0",
    "@rspack/core": "^0.7.0",
    "@testing-library/react": "^15.0.7",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.2.5",
